        "app.ts",
        "config/index.ts",
        "config/vars.ts",
        "index.ts",
        "namespace.ts",
        "provider.ts",
        "queue.ts",
        "ruleAmqp.ts",
        "ruleAmqpExternal.ts",
        "ruleAzureFunction.ts",
        "ruleCloudflareWorker.ts",
        "ruleGoogleFunction.ts",
        "ruleHttp.ts",
        "ruleIfttt.ts",
        "ruleKafka.ts",
        "ruleKinesis.ts",
        "ruleLambda.ts",
        "rulePulsar.ts",
        "ruleSqs.ts",
        "ruleZapier.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
