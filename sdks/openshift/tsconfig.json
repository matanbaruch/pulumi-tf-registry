{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "buildConfig.ts",
        "config/index.ts",
        "config/vars.ts",
        "deploymentConfig.ts",
        "imageStream.ts",
        "index.ts",
        "netnamespace.ts",
        "project.ts",
        "projectRequest.ts",
        "provider.ts",
        "route.ts",
        "secret.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
