{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "bytes.ts",
        "id.ts",
        "index.ts",
        "integer.ts",
        "password.ts",
        "pet.ts",
        "provider.ts",
        "shuffle.ts",
        "string.ts",
        "utilities.ts",
        "uuid.ts"
    ]
}
