{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "config/index.ts",
        "config/vars.ts",
        "gitCommitPush.ts",
        "gitReset.ts",
        "githubPr.ts",
        "index.ts",
        "provider.ts",
        "stateMigration.ts",
        "terraformInit.ts",
        "terraformPlan.ts",
        "updateBackend.ts",
        "utilities.ts"
    ]
}
