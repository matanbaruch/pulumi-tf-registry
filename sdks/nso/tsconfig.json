{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "config/index.ts",
        "config/vars.ts",
        "device.ts",
        "deviceConfig.ts",
        "deviceGroup.ts",
        "getDevice.ts",
        "getDeviceConfig.ts",
        "getDeviceGroup.ts",
        "getRestconf.ts",
        "index.ts",
        "provider.ts",
        "restconf.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
