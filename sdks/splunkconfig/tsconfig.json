{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "appAutoVersion.ts",
        "appPackage.ts",
        "config/index.ts",
        "config/vars.ts",
        "getAppAttributes.ts",
        "getAppIds.ts",
        "getAppPackage.ts",
        "getIndexAttributes.ts",
        "getIndexNames.ts",
        "getLookupAttributes.ts",
        "getRoleAttributes.ts",
        "getRoleNames.ts",
        "getSamlGroupAttributes.ts",
        "getSamlGroupNames.ts",
        "getUserAttributes.ts",
        "getUserNames.ts",
        "index.ts",
        "provider.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
