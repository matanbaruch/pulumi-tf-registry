        "index.ts",
        "provider.ts",
        "record.ts",
        "smartRulesFirewall.ts",
        "smartRulesRewrite.ts",
        "smartRulesSettings.ts",
        "sslCertificate.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
