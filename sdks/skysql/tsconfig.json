        "getAvailabilityZones.ts",
        "getCredentials.ts",
        "getProjects.ts",
        "getService.ts",
        "getVersions.ts",
        "index.ts",
        "provider.ts",
        "service.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
