{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "config/index.ts",
        "config/vars.ts",
        "getS3Bucket.ts",
        "getS3Object.ts",
        "index.ts",
        "permission.ts",
        "provider.ts",
        "s3bucket.ts",
        "s3bucketObjectLockConfiguration.ts",
        "s3object.ts",
        "s3objectCopy.ts",
        "serviceAccount.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
