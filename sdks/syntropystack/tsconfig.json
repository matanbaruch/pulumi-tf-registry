{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "agent.ts",
        "config/index.ts",
        "config/vars.ts",
        "getAgent.ts",
        "getAgentSearch.ts",
        "getNetworkConnectionServices.ts",
        "index.ts",
        "networkConnection.ts",
        "networkConnectionMesh.ts",
        "networkConnectionServices.ts",
        "provider.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
