{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "config/index.ts",
        "config/vars.ts",
        "contentRuleServers.ts",
        "contentRules.ts",
        "index.ts",
        "letsencryptCertificate.ts",
        "provider.ts",
        "securityPolicies.ts",
        "selfSignedCertificate.ts",
        "servers.ts",
        "services.ts",
        "signedCertificate.ts",
        "trustedCaCertificate.ts",
        "trustedServerCertificate.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
