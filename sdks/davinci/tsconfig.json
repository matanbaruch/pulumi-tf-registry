{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "application.ts",
        "applicationFlowPolicy.ts",
        "config/index.ts",
        "config/vars.ts",
        "connection.ts",
        "flow.ts",
        "getApplication.ts",
        "getApplications.ts",
        "getConnection.ts",
        "getConnections.ts",
        "index.ts",
        "provider.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts",
        "variable.ts"
    ]
}
